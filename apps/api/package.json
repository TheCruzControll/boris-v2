{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format cjs --dts --watch --onSuccess \"node dist/index.js\"",
    "seed-db": "tsup src/scripts/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "tsup src/index.ts --format cjs --onSuccess \"node dist/index.js\"",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "@fightmegg/riot-api": "^0.0.8",
    "body-parser": "^1.19.0",
    "canvas": "^2.11.0",
    "chance": "^1.1.11",
    "cors": "^2.8.5",
    "database": "workspace:*",
    "discord.js": "^14.8.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "morgan": "^1.10.0",
    "node-emoji": "^1.11.0",
    "pagination.djs": "^4.0.9",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chance": "^1.1.3",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.2.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/node-emoji": "^1.8.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "workspace:*",
    "jest": "^26.6.3",
    "jest-presets": "workspace:*",
    "supertest": "^6.2.4",
    "tsconfig": "workspace:*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  }
}
