{
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo run build",
    "build:boris": "dotenv -- turbo run build --filter=boris",
    "build:boris-site": "dotenv -- turbo run build --filter=boris-site",
    "clean": "turbo run clean",
    "dev": "dotenv -- turbo run dev --no-cache  --continue",
    "dev:boris": "dotenv -- turbo run dev --no-cache  --continue --filter=boris",
    "dev:boris-site": "dotenv -- dotenv -- turbo run dev --no-cache  --continue --filter=boris-site",
    "start:boris": "dotenv -- turbo run start --filter=boris",
    "start:boris-site": "dotenv -- turbo run start --filter=boris-site",
    "db:generate": "dotenv -- turbo run db:generate",
    "db:push": "dotenv -- turbo run db:push",
    "seed-db": "dotenv -- turbo run seed-db",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0",
  "dependencies": {
    "dotenv-cli": "^7.1.0"
  }
}
